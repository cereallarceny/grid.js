{
  "name": "@openmined/grid.js",
  "version": "0.0.0",
  "description": "A Node socket API supporting syft.js workers in the browser",
  "main": "src/index.js",
  "scripts": {
    "start": "VERBOSE=true nodemon --exec babel-node src/index.js",
    "build": "babel src --out-dir dist",
    "serve": "node dist/index.js",
    "test": "jest --coverage --maxWorkers 4",
    "test:watch": "jest --watch",
    "seed": "./seed/run-seed.sh",
    "create-seed": "python ./seed/create-seed.py"
  },
  "engines": {
    "node": "10.16.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/openmined/grid.js.git"
  },
  "author": "OpenMined",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/openmined/grid.js/issues"
  },
  "homepage": "https://github.com/openmined/grid.js#readme",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@openmined/syft.js": "0.0.1-0",
    "bcrypt": "^3.0.7",
    "jsonwebtoken": "^8.5.1",
    "mongodb": "^3.4.1",
    "redis": "^2.8.0",
    "uuid": "^3.3.3",
    "ws": "^7.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.7",
    "@babel/core": "^7.7.7",
    "@babel/node": "^7.7.7",
    "@babel/plugin-transform-runtime": "^7.7.6",
    "@babel/preset-env": "^7.7.7",
    "babel-core": "^6.26.3",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "mock-socket": "^9.0.2",
    "mongodb-memory-server": "^6.2.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "redis-mock": "^0.47.0"
  }
}
